<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="WindowBackground" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="TextColor" Color="#111111"/>
    <SolidColorBrush x:Key="StatusBarBackground" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="BorderColor" Color="#FFFFFF"/>

    <SolidColorBrush x:Key="ButtonBackground" Color="#0078D4"/>
    <SolidColorBrush x:Key="ButtonHover" Color="#005A9E"/>
    <SolidColorBrush x:Key="ButtonPressed" Color="#004578"/>
    <SolidColorBrush x:Key="ButtonForeground" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="ButtonBorder" Color="#005A9E"/>

    <SolidColorBrush x:Key="AddBackground" Color="#107C10"/>
    <SolidColorBrush x:Key="AddHover" Color="#0B5A0B"/>
    <SolidColorBrush x:Key="AddPressed" Color="#083B08"/>

    <SolidColorBrush x:Key="DeleteBackground" Color="#D13438"/>
    <SolidColorBrush x:Key="DeleteHover" Color="#A4262C"/>
    <SolidColorBrush x:Key="DeletePressed" Color="#7A1D20"/>
    <SolidColorBrush x:Key="ListBackground" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="ListItemHover" Color="#EAEAEA"/>
    <SolidColorBrush x:Key="ListItemSelected" Color="#D6D6D6"/>
    <SolidColorBrush x:Key="MenuBackground" Color="#EDEDED"/>
    <SolidColorBrush x:Key="MenuForeground" Color="#111111"/>
    <SolidColorBrush x:Key="MenuHover" Color="#DCDCDC"/>
    <SolidColorBrush x:Key="MenuPressed" Color="#C8C8C8"/>
    <SolidColorBrush x:Key="MenuBorder" Color="#B5B5B5"/>

</ResourceDictionary>
